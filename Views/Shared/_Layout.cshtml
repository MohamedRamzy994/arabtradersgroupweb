<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ArabTradersGroup.App_LocalResources.ATG.arabtradersgroup</title>
    <!-- Stylesheets -->
    <link href="@ArabTradersGroup.App_LocalResources.ATG.cssbootstrapu" rel="stylesheet">
    <link href="@ArabTradersGroup.App_LocalResources.ATG.cssrevolutionu" rel="stylesheet">
    <link href="@ArabTradersGroup.App_LocalResources.ATG.cssstyleu" rel="stylesheet">
    <link href="@ArabTradersGroup.App_LocalResources.ATG.cssflaticonu" rel="stylesheet">
    <link rel="shortcut icon" href="~/Images/images/favicon.ico" type="image/x-icon">
    <link rel="icon" href="~/Images/images/favicon.ico" type="image/x-icon">
    <!-- Responsive -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link href="@ArabTradersGroup.App_LocalResources.ATG.cssresponsiveu" rel="stylesheet">
    <!--[if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.js"></script><![endif]-->
    <!--[if lt IE 9]><script src="js/respond.js"></script><![endif]-->
    @*@Styles.Render("~/Content/css")*@
    <link rel="stylesheet" href="@ArabTradersGroup.App_LocalResources.ATG.cssfontu">


    @Scripts.Render("~/bundles/modernizr")

</head>
<body>


    <div class="page-wrapper">

        <!-- Preloader -->
        <div class="preloader"></div>

        <!-- Main Header-->
        <header class="main-header">

            <!-- Main Box -->
            <div class="main-box">
                <div class="auto-container">
                    <div class="outer-container clearfix">
                        <!--Logo Box-->
                        <div class="logo-box">
                            <div class="logo"><a href="@Url.Action("Index","Home",null)"><img src="~/Images/images/ATGLogo.jpg" alt=""></a></div>
                        </div>

                        <!--Nav Outer-->
                        <div class="nav-outer clearfix">
                            <!-- Main Menu -->
                            <nav class="main-menu">
                                <div class="navbar-header">
                                    <!-- Toggle Button -->
                                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>
                                </div>

                                <div class="navbar-collapse collapse clearfix">
                                    <ul class="navigation clearfix">
                                        <li class="dropdown @ViewBag.currenthome"><a href="@Url.Action("Index","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.home</a></li>
                                        <li class="dropdown @ViewBag.currentabout"><a href="@Url.Action("About","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.aboutus</a></li>
                                        <li class="dropdown @ViewBag.currentproduct">
                                            <a href="@Url.Action("Products","Home")">@ArabTradersGroup.App_LocalResources.ATG.product</a>
                                           
                                        </li>
                                        <li class="dropdown @ViewBag.currentgalley"><a href="@Url.Action("Gallery","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.ugallery</a></li>
                                        <li class="dropdown @ViewBag.currentfaq"><a href="@Url.Action("Faq","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.ufaq</a></li>

                                        <li class="dropdown @ViewBag.currentcontact"><a href="@Url.Action("Contact","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.contactus</a></li>
                                        @Html.Partial("_LoginPartial")
                                    </ul>


                                </div>
                            </nav><!-- Main Menu End-->

                        </div><!--Nav Outer End-->
                        <!-- Hidden Nav Toggler -->
                        <div class="nav-toggler">
                            <button class="hidden-bar-opener"><span class="icon fa fa-bars"></span></button>
                        </div><!-- / Hidden Nav Toggler -->

                    </div>
                </div>
            </div>

        </header>
        <!--End Main Header -->
        <!-- Hidden Navigation Bar -->
        <section class="hidden-bar right-align">

            <div class="hidden-bar-closer">
                <button class="btn"><i class="fa fa-close"></i></button>
            </div>

            <!-- Hidden Bar Wrapper -->
            <div class="hidden-bar-wrapper">

                <!-- .logo -->
                <div class="logo text-center">
                    <a href="index.html"><img src="~/Images/images/ATGLogo.jpg" alt=""></a>
                </div><!-- /.logo -->
                <!-- .Side-menu -->
                <div class="side-menu">
                    <!-- .navigation -->
                    <ul class="navigation">
                        <li class="dropdown @ViewBag.currenthome"><a href="@Url.Action("Index","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.home</a></li>
                        <li class="dropdown @ViewBag.currentabout"><a href="@Url.Action("About","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.aboutus</a></li>
                        <li class="dropdown @ViewBag.currentproduct">
                            <a href="@Url.Action("Products","Home")">@ArabTradersGroup.App_LocalResources.ATG.product</a>

                        </li>
                        <li class="dropdown @ViewBag.currentgalley"><a href="@Url.Action("Gallery","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.ugallery</a></li>
                        <li class="dropdown @ViewBag.currentfaq"><a href="@Url.Action("Faq","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.ufaq</a></li>

                        <li class="dropdown @ViewBag.currentcontact"><a href="@Url.Action("Contact","Home",null)">@ArabTradersGroup.App_LocalResources.ATG.contactus</a></li>
                        @Html.Partial("_LoginPartial")
                    </ul>
                </div><!-- /.Side-menu -->

                <div class="social-icons">
                    <ul>
                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                        <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                    </ul>
                </div>

            </div><!-- / Hidden Bar Wrapper -->
        </section>
        <!-- / Hidden Bar -->
        <!-- Body Container Placeholder -->
        <div class="container body-content">
            @RenderBody()
        </div>
        <!-- / Body Container Placeholder -->
        <!--Main Footer / Footer Style One-->
        <footer class="main-footer footer-style-one">

            <!--Footer Upper-->
            <div class="footer-upper">
                <div class="auto-container">
                    <div class="row clearfix">

                        <!--Footer Column-->
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 column">
                            <div class="footer-widget about-widget">
                                @*<figure class="footer-logo"><img src="~/Images/images/ATGLogo.jpg" alt=""></figure>*@
                                <h2>@ArabTradersGroup.App_LocalResources.ATG.arabtradersgroup</h2>

                                <p class="text-justify">

                                    @ArabTradersGroup.App_LocalResources.ATG.aboutustab1paragraph

                                </p>



                            </div>
                        </div>

                        <!--Footer Column-->
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 column">
                            <div class="footer-widget info-widget">
                                <h2>@ArabTradersGroup.App_LocalResources.ATG.keepintouch</h2>
                                <ul class="info">
                                    <li>@ArabTradersGroup.App_LocalResources.ATG.footeraddress:@ArabTradersGroup.App_LocalResources.ATG.footeraddressdata </li>
                                    <li>@ArabTradersGroup.App_LocalResources.ATG.footerphone :  @ArabTradersGroup.App_LocalResources.ATG.footerphonedata</li>
                                    <li>@ArabTradersGroup.App_LocalResources.ATG.footeremail : @ArabTradersGroup.App_LocalResources.ATG.footeremailedata</li>
                                    <li>@ArabTradersGroup.App_LocalResources.ATG.footerfax : @ArabTradersGroup.App_LocalResources.ATG.footerfaxedata</li>
                                </ul>
                            </div>
                        </div>

                        <!--Footer Column-->
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12 column padd-left-60">
                            <div class="footer-widget links-widget">
                                <h2>@ArabTradersGroup.App_LocalResources.ATG.footerlinks</h2>
                                <div class="row clearfix">
                                    <div class="col-md-12 col-sm-6 col-xs-12">
                                        <ul>
                                            <li><a href="@Url.Action("About","Home")">@ArabTradersGroup.App_LocalResources.ATG.aboutus</a></li>
                                            <li><a href="@Url.Action("Gallery","Home")"> @ArabTradersGroup.App_LocalResources.ATG.ugallery</a></li>
                                            <li><a href="@Url.Action("Products","Home")"> @ArabTradersGroup.App_LocalResources.ATG.products</a></li>
                                        </ul>
                                    </div>

                                  
                                </div>
                                <br>
                                <div class="social-links">
                                    <h3>@ArabTradersGroup.App_LocalResources.ATG.followus</h3>
                                    <div class="links">
                                        <a href="https://www.facebook.com/profile.php?id=100003389195478" target="_blank"><span class="fa fa-facebook-official"></span></a>
                                        <a href="https://twitter.com/arabtraders4" target="_blank"><span class="fa fa-twitter"></span></a>
                                       
                                        <a href="https://www.instagram.com/arabtradersgroup/" target="_blank"><span class="fa fa-instagram"></span></a>
                                      
                                    </div>

                                </div>

                            </div>
                        </div>


                    </div>

                </div>
            </div>

            <!--Footer Bottom-->
            <div class="footer-bottom">
                <div class="auto-container">

                    <!--Copyright-->
                    <div class="copyright text-info">@ArabTradersGroup.App_LocalResources.ATG.footercopyright</div>
                    <div class="text-danger">@ArabTradersGroup.App_LocalResources.ATG.footerappletsoftware </div>

                </div>
            </div>

        </footer>

        <!-- / Main Footer -->


    </div>
    <!--End pagewrapper-->
    <!--Scroll to top-->
    <div class="scroll-to-top scroll-to-target" data-target="html"><span class="fa fa-long-arrow-up"></span></div>


    <script src="~/Scripts/js/jquery.js"></script>
    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <script src="~/Scripts/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Scripts/js/revolution.min.js"></script>
    <script src="~/Scripts/js/jquery.fancybox.pack.js"></script>
    <script src="~/Scripts/js/jquery.fancybox-media.js"></script>
    <script src="~/Scripts/js/mixitup.js"></script>
    <script src="~/Scripts/js/owl.js"></script>
    <script src="~/Scripts/js/wow.js"></script>
    <script src="~/Scripts/jsadmin/jquery.nicescroll.min.js"></script>

    <script>

        $("html").niceScroll();

    </script>


    @RenderSection("scripts", required: false)

    <script src="~/Scripts/js/script.js"></script>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

</body>
</html>
